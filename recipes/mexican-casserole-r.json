{
    "title": "Mexican Casserole",
    "bake": "350 degrees for 45 minutes",
    "ingredients": [
        {   
            "type": "chips",
            "ingredient": "Tortilla Chips",
            "amount": "2 cups"
        },
        {   
            "type": "meat",
            "ingredient": "Ground beef",
            "amount": "1 lb"
        },
        {   
            "type": "dairy",
            "ingredient": "Fiesta Cheese",
            "amount": "1 cup"
        },
        {   
            "type": "can",
            "ingredient": "Chili beans (Dennison Original)",
            "amount": "1 15oz can"
        },
        {   
            "type": "can",
            "ingredient": "Diced tomatoes",
            "amount": "1 can"
        },
        {   
            "type": "dairy",
            "ingredient": "Sour Cream",
            "amount": "1 cup"
        },
        {   
            "type": "produce",
            "ingredient": "Green Onions",
            "amount": "1/2 cup"
        }
    ],
    "directions": [
        {
            "step": "1.",
            "process": "Spread crushed chips on bottom of 13x9 dish"
        },
        {
            "step": "2.",
            "process": "Cook ground beef until brown, then add beans, tomatoes, & green onions in cooking pan."
        },
        {
            "step": "3.",
            "process": "Spread over the chips."
        },
        {
            "step": "4.",
            "process": "Spread sour cream and cheese over top and put in oven."
        }
    ]
}