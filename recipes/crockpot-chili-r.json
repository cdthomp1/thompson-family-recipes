{
    "title": "Crockpot-Chili",
    "author": "Cameron Thompson",
    "image": "https://cdn.pixabay.com/photo/2014/06/28/14/14/chili-con-carne-378952_960_720.jpg",
    "bake": "n/a",
    "ingredients": [
        {   
            "type": "can",
            "ingredient": "black beans",
            "amount": "1 15oz can"
        },
        {   
            "type": "can",
            "ingredient": "can corn, undrained",
            "amount": "1 15.25oz"
        },
        {   
            "type": "can",
            "ingredient": "Rotel tomatoes, undrained",
            "amount": "1 10oz can"
        },
        {   
            "type": "mixes",
            "ingredient": "ranch dressing mix",
            "amount": "1 pkg"
        },
        {   
            "type": "spices",
            "ingredient": "Cumin",
            "amount": "1 tsp"
        },
        {   
            "type": "spices",
            "ingredient": "Chili powder",
            "amount": "1 tbsp"
        },
        {   
            "type": "spices",
            "ingredient": "Onion powder",
            "amount": "1 tsp"
        },
        {   
            "type": "dairy",
            "ingredient": "light cream cheese",
            "amount": "1 8oz pkg"
        },
        {   
            "type": "meat",
            "ingredient": "chicken breasts",
            "amount": "2"
        }
    ],
    "directions": [
        {
            "step": "1",
            "process": "Drain and rinse the black beans. Place chicken at the bottom of the crock pot, then pour out the whole can of corn (undrained), Rotel tomatoes, and black beans on top of chicken."
        },
        {
            "step": "2",
            "process": "Top with seasonings and ranch dressing mix. Stir together."
        },
        {
            "step": "3",
            "process": "Place cream cheese block on top. Cover with lid and cook on low for 6-8 hours."
        },
        {
            "step": "4",
            "process": "After the cooking time is over, take chicken breasts from crock pot and shred and add back to the chilli. Stir together and enjoy!"
        }
    ]
}