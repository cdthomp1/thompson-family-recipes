{
    "title": "Macaroni and Cheese",
    "bake": "400 degrees for 25-30 minutes",
    "ingredients": [
        {   
            "type": "pasta",
            "ingredient": "Elbow macaroni",
            "amount": "1 (16 oz) Package"
        },
        {   
            "type": "dairy",
            "ingredient": "Butter",
            "amount": "6 Tbsp"
        },
        {   
            "type": "Baking",
            "ingredient": "Flour",
            "amount": "6 Tbsp"
        },
        {   
            "type": "spice",
            "ingredient": "Mustard, Ground",
            "amount": "2 tbsp"
        },
        {   
            "type": "spice",
            "ingredient": "Pepper",
            "amount": "1.5 tsp"
        },
        {   
            "type": "spice",
            "ingredient": "Garlic Powder",
            "amount": "1 tsp"
        },
        {   
            "type": "spice",
            "ingredient": "Salt",
            "amount": "1 tsp"
        },
        {   
            "type": "dairy",
            "ingredient": "Milk",
            "amount": "4 cups "
        },
        {   
            "type": "dairy",
            "ingredient": "Shredded Sharp Cheddar Cheese",
            "amount": "6 cups"
        },
        {   
            "type": "baking",
            "ingredient": "Panko bread crumbs",
            "amount": "1.5 cups "
        },
        {   
            "type": "spice",
            "ingredient": "Paprika",
            "amount": "1 tsp"
        }
    ],
    "directions": [
        {
            "step": "1.",
            "process": "Preheat oven 400â„‰. Cook macaroni in large pot as directed on package. Rinse under cold water."
        },
        {
            "step": "2.",
            "process": "Melt butter in same saucepan on medium heat. Sprinkle with flour and seasonings. Cook and stir 2 minutes or until well blended. Gradually stir in milk until smooth. Stirring constantly, cook 3 minutes or until sauce starts to thicken. Stir in cheese until melted and smooth. Remove from heat."
        },
        {
            "step": "3.",
            "process": "Add macaroni; toss gently to coat. Pour into greased 13x9 dish."
        },
        {
            "step": "4.",
            "process": "Mix panko and paprika; sprinkle evenly over the top."
        },
        {
            "step": "5.",
            "process": "Bake 25-30 minutes or until bubbly and golden brown on top. Let stand 5 minutes before serving."
        }
    ]
}