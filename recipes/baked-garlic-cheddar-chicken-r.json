{
    "title": "Baked Garlic Cheddar Chicken",
    "bake": "350 degrees for approx. 30 minutes",
    "ingredients": [
        {   
            "type": "dairy",
            "ingredient": "butter",
            "amount": "1/2 cup"
        },
        {   
            "type": "produce",
            "ingredient": "garlic",
            "amount": "4 cloves"
        },
        {   
            "type": "baking",
            "ingredient": "bread crumbs",
            "amount": "3/4 cups"
        },
        {   
            "type": "dairy",
            "ingredient": "parmesan cheese (grated)",
            "amount": "1/2 cup"
        },
        {   
            "type": "dairy",
            "ingredient": "cheddar cheese",
            "amount": "1.5 cups"
        },
        {   
            "type": "spice",
            "ingredient": "dried oregano",
            "amount": "1/4 tsp"
        },
        {   
            "type": "spice",
            "ingredient": "dried parsley",
            "amount": "1/4 tsp"
        },
        {   
            "type": "spice",
            "ingredient": "black pepper",
            "amount": "1/4 tsp"
        },
        {   
            "type": "spice",
            "ingredient": "salt",
            "amount": "1/8 tsp"
        },
        {   
            "type": "meat",
            "ingredient": "boneless chicken breasts",
            "amount": "8 pieces"
        }
    ],
    "directions": [
        {
            "step": "1.",
            "process": "Preheat oven o 350 degrees F. Grease a 13x9 inch baking dish and cover the bottom with a thin layer of bread crumbs."
        },
        {
            "step": "2.",
            "process": "Melt butter in a small saucepan over low heat and cook garlic until tender, about 5 minutes."
        },
        {
            "step": "3.",
            "process": "In a shallow bowl, mix together breadcrumbs, parmesan cheese, cheddar cheese, parsley, oregano, pepper, salt. Dip each chicken breast in garlic butter to coat, then press into the bread crumb mixture. Place coated chicken breasts in a prepared baking dish. Drizzle with any remaining butter and sprinkle with any remaining bread crumb mixture."
        },
        {
            "step": "4.",
            "process": "Bake for 30 minutes or until chicken is no longer pink and juices run clear."
        }
    ]
}