{
    "title": "Sausage, Pepper, and Rice Skillet",
    "author": "",
    "image": "",
    "bake": "",
    "ingredients": [
        {   
            "type": "meat",
            "ingredient": "Package Sausage, smoked",
            "amount": "1 (12oz)"
        },
        {   
            "type": "produce",
            "ingredient": "Garlic",
            "amount": "4 cloves"
        },
        {   
            "type": "spice",
            "ingredient": "Parsley",
            "amount": "1 1/2 tbsp"
        },
        {   
            "type": "produce",
            "ingredient": "Red bell pepper",
            "amount": "1/2"
        },
        {   
            "type": "produce",
            "ingredient": "White onion",
            "amount": "1 small"
        },
        {   
            "type": "produce",
            "ingredient": "Yellow bell pepper",
            "amount": "1/2"
        },
        {   
            "type": "can",
            "ingredient": "Chicken broth, low-sodium",
            "amount": "1 1/4 cups"
        },
        {   
            "type": "can",
            "ingredient": "Tomato paste",
            "amount": "5 tbsp"
        },
        {   
            "type": "grains",
            "ingredient": "White rice",
            "amount": "1 1/4 cups"
        },
        {   
            "type": "spice",
            "ingredient": "black pepper",
            "amount": "1/2 tsp"
        },
        {   
            "type": "spice",
            "ingredient": "Cayenne pepper",
            "amount": "1/8 tsp"
        },
        {   
            "type": "spice",
            "ingredient": "Salt",
            "amount": "1/2 tsp"
        },
        {   
            "type": "spice",
            "ingredient": "Paprika",
            "amount": "1 tsp"
        },
        {   
            "type": "oil",
            "ingredient": "Olive Oil",
            "amount": "2 tsp"
        }
    ],
    "directions": [
        {
            "step": "1.",
            "process": "In a small saucepan, cook rice according to the package’s directions. "
        },
        {
            "step": "2.",
            "process": "Place a large cast iron skillet over medium-hight heat. Once the skillet is hot, add the oil. After the oil shimmers, add the sausage and cook until browned on both sides, about 5 minutes. Remove from the pan and set aside. "
        },
        {
            "step": "3.",
            "process": "Add the peppers and onion, saute for 4-5 minutes. Add the garlic, salt, and pepper, cook until fragrant, about 1 minute. Remove from the pan and set aside with the sausage."
        },
        {
            "step": "4.",
            "process": "Add the tomato paste and about ¾ cup of chicken broth, whisk to combine. Allow the mixture to simmer for 1 minute, then add the paprika and cayenne."
        },
        {
            "step": "5.",
            "process": "Stir in the cooked rice, sausage, remaining chicken broth, pepper and onions until combined. Garnish with chopped parsley, serve immediately."
        }
    ]
}